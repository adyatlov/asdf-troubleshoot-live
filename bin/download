#!/usr/bin/env bash

set -e
DOWNLOAD_FILE_NAME=v${ASDF_INSTALL_VERSION}.tar.gz

curl -L -o ${ASDF_DOWNLOAD_PATH}/${DOWNLOAD_FILE_NAME} https://github.com/mhrabovcin/troubleshoot-live/archive/refs/tags/${DOWNLOAD_FILE_NAME}
tar -xzvf ${ASDF_DOWNLOAD_PATH}/${DOWNLOAD_FILE_NAME}  -C ${ASDF_DOWNLOAD_PATH}
